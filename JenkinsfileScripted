@Library('scripted-shared-lib') _
def url = scm.userRemoteConfigs[0].url  
def branchUrl = scm.branches[0].name
def branch = "${GIT_BRANCH.split("/")[1]}"
println(url+"   "+branchUrl +"   "+branch)
def conf = ['url':url,'branch':branchUrl]
scriptedSL.call(conf)
println(conf)
scriptedSL.buildAndDeploy(conf)
